{
    "manifest_version": 2,

    "name": "GitHub-CN",
    "description": "GitHub汉化版23333",
    "version": "1.0",

    "icons": {
        "16": "logo-16.png",
        "32": "logo-32.png",
        "48": "logo-48.png",
        "128": "logo-128.png"
    },
    "content_scripts": [{
        "all_frames": true,
        "js": ["inject.js"],
        "matches": ["*://*.github.com/*"],
        "run_at": "document_end"
    }]
}
